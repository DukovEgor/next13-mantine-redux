{
	"trailingComma": "all",
	"tabWidth": 3,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/pages(.*)$",
		"^@/components(.*)$",
		"^@/hooks/(.*)$",
		"^@/store/(.*)$",
		"^@/router/(.*)$",
		"^@/api/(.*)$",
		"^@/middlewares/(.*)$",
		"^@/constant/(.*)$",
		"^@/types/(.*)$",
		"^@/utils/(.*)$",
		"^../(.*)$",
		"^./(.*)$",
		"^(.less)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
